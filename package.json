{
  "name": "frozen-food-backend",
  "version": "1.0.0",
  "description": "Complete REST API for Frozen Food Directory with MongoDB, JWT Auth, and Swagger Docs",
  "type": "module",
  "main": "src/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/frozen-food-backend.git"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "seed:admin": "node src/scripts/seedAdmin.mjs",
